<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE EVENT [
  <!ELEMENT EVENT (#PCDATA)>
  <!ENTITY nbsp '&#160;'>
  <!ENTITY cr '&#13;'>
  <!ENTITY lf '&#10;'>
]>

<EVENT TYPE="UPDATE">
  <DESCRIPTION>Descrição do event</DESCRIPTION>
  <CONDITION>Condição</CONDITION>
  <WIOBJ>Objeto de retorno</WIOBJ>
  <PREFIX>Prefixo utilizado em update múltiplo</PREFIX>
  <SIZE>Quantidade de registros para update múltiplo</SIZE>
  <ROWCONDITION>Condição para cada linha, caso múltiplo</ROWCONDITION>
  <OBJECTID>Objeto utilizado para cada linha</OBJECTID>
  <DATABASE>Banco de dados a ser utilizado</DATABASE>
  <SQL>Expressão SQL</SQL>
  <SQLFILTER>Caracteres a serem removidos da expressão SQL</SQLFILTER>
  <MSGFALSE>
    <MESSAGE CODE="1">Mensagem para o erro 1</MESSAGE>
    <MESSAGE CODE="2">Mensagem para o erro 2</MESSAGE>
    <NOMESSAGE>Retorna em caso de erro quando não há MessageFalse adequada</NOMESSAGE>
  </MSGFALSE>
  <MSGTRUE>Mensagem caso seja efetuado corretamente</MSGTRUE>
  <AUTOCOMMIT>ON/OFF - Liga ou desliga auto-commit</AUTOCOMMIT>
</EVENT>
